    "RTI-01": {
      "form_id": "RTI-01",
      "form_name": "Information Application Form",
      "form_title": "Application to receive Information",
      "organization": "National Aquatic Resources Research and Development Agency (NARA)",
      "logo": "nara-logo.jpg",
      "note": "Note that completion of this Form is not a compulsory requirement to make a request for information and that any written letter, e-mail or a verbal request with the essential information to identify the requested information is sufficient.",
      "recipient": "Information Officer",
      "fields": [
        {
          "field_number": "01",
          "field_name": "name_of_requestor",
          "field_label": "Name of Requestor",
          "field_type": "text",
          "required": true
        },
        {
          "field_number": "02",
          "field_name": "address",
          "field_label": "Address",
          "field_type": "textarea",
          "required": true
        },
        {
          "field_number": "03",
          "field_name": "contact_no",
          "field_label": "Contact No. (if any)",
          "field_type": "text",
          "required": false
        },
        {
          "field_number": "04",
          "field_name": "email_address",
          "field_label": "Email Address (if any)",
          "field_type": "email",
          "required": false
        },
        {
          "field_number": "05",
          "field_name": "details_about_information_requested",
          "field_label": "Details about Information requested",
          "field_type": "group",
          "subfields": [
            {
              "subfield_id": "I",
              "subfield_name": "specific_public_authority",
              "subfield_label": "Specific Public Authority",
              "field_type": "text"
            },
            {
              "subfield_id": "II",
              "subfield_name": "information_requested",
              "subfield_label": "Information requested",
              "field_type": "textarea"
            },
            {
              "subfield_id": "III",
              "subfield_name": "specific_period",
              "subfield_label": "Specific period information is requested (if applicable)",
              "field_type": "text"
            }
          ]
        },
        {
          "field_number": "06",
          "field_name": "manner_information_requested",
          "field_label": "Manner in which information is requested",
          "field_type": "checkbox",
          "options": [
            "inspect relevant work, documents, records",
            "Take notes, extracts or certified copies of documents or records",
            "Take certified samples of material",
            "Obtain information in the form of CD/DVD/storage media/tapes/video cassettes/ any other electronic mode/ printouts where such information is stored in a computer or in any other device"
          ]
        },
        {
          "field_number": "07",
          "field_name": "language_preference",
          "field_label": "Language in which requestor prefers access",
          "field_type": "select",
          "options": [
            "Sinhala",
            "Tamil",
            "English"
          ]
        },
        {
          "field_number": "08",
          "field_name": "life_liberty_concern",
          "field_label": "Does the information request concern the life and personal liberty of a citizen?",
          "field_type": "radio",
          "options": [
            "Yes",
            "No"
          ],
          "conditional": {
            "if_yes": {
              "field_name": "reason_for_belief",
              "field_label": "If Yes, give reason for belief",
              "field_type": "textarea"
            }
          }
        },
        {
          "field_number": "09",
          "field_name": "other_details",
          "field_label": "Any other details",
          "field_type": "textarea",
          "required": false
        },
        {
          "field_number": "10",
          "field_name": "relevant_documents",
          "field_label": "Relevant documents attached (If any)",
          "field_type": "list",
          "max_items": 3
        },
        {
          "field_number": "11",
          "field_name": "sri_lankan_citizen",
          "field_label": "Is the requestor a citizen of Sri Lanka?",
          "field_type": "radio",
          "options": [
            "Yes",
            "No"
          ],
          "required": true
        }
      ],
      "signature_section": {
        "date": "Date",
        "signature": "Signature"
      }
    },
